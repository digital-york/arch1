<div class="popup popup_subject">

  <div class="popup_title">'<%= @subject_name %>'</div>

  <div style="margin-top: 10px">The subject '<%= @subject_name %>'  exists in one or more entries. Please remove it from the records displayed below (using the main interface)
    <%= link_to 'then click here to go back and delete the subject', subject_path(@go_back_id), title: 'Go back' %>.
  </div>

  <div style="margin-top: 10px; margin-left: 0px">
    <% @existing_location_list.each do |t| %>
        <div style="margin-top: 5px">-&nbsp;&nbsp;<%= t %></div>
    <% end %>
  </div>
</div>