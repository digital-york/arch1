<html lang="en" class="no-js">

<head>
  <meta charset="utf-8"/>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <title>Archbishops&#39; Registers</title>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "application" %>
</head>

<body>

<div id="container">

  <div id="header" style="border-bottom: 1px solid #898989">

    <div id="header_text">
      Archbishops&#39; Registers Editing Tool
    </div>

    <div style="float: right; margin-right: 6px; margin-top: 25px">
      <%= button_to 'Logout', logout_path, :method => 'get', :data => {:confirm => 'Are you sure you want to logout?'} %>
    </div>

  </div>

  <!--<div id="navbar"></div>-->

  <div id="main" style="margin-top: 10px">
    <%= yield %>
  </div>

  <div style="clear: both"></div>

</div>

</body>
</html>

